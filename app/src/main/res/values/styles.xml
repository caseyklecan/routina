<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="ButtonStyle" parent="Theme.AppCompat.Light">
        <item name="android:buttonStyle">
            @style/Widget.AppCompat.Button.Borderless.Colored
        </item>
        <item name="colorAccent">#00A1CA</item>
    </style>

    <color name="colorLarge">@color/colorDark</color>
    <dimen name="fontReg">16sp</dimen>
    <dimen name="fontLarge">24sp</dimen>
    <dimen name="fontMed">20sp</dimen>

    <dimen name="marginBetween">4dp</dimen>
    <dimen name="marginAround">16dp</dimen>

</resources>
